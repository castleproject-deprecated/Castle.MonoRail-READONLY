<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="castle">
			<section name="mvc" type="Castle.MVC.Configuration.ConfigurationSectionHandler, castle.mvc"/>
		</sectionGroup>
	</configSections>
	<castle> 
		<mvc>
			<webViews>
				<view id="page2" path="/views/home/page2.aspx" />
				<view id="index" path="/views/home/index.aspx"  />
				<view id="error"  path="/views/error.aspx" />
			</webViews>

			<command-mappings>
				<commands view="index">
					<command id="Login" view="page2" />
				</commands>
				<commands view="cart">
					<command id="resume" view="browsecatalog" />
					<command id="checkout" view="checkout" />
					<command id="fail" view="error" />
					<command id="stop" view="logon"/>
				</commands>
			</command-mappings>
		</mvc>
	</castle> 
	
	<appSettings>
		<!-- <add key="jey" value="value"/> -->
	</appSettings>
	

	
</configuration>
