<?xml version="1.0" encoding='iso-8859-1' ?>
<project
	name="castle-dynamicproxy2"
	default="build"
	xmlns="http://nant.sf.net/release/0.85/nant.xsd"
>

	<property name="root.dir" value="../.." />
	<include buildfile="${root.dir}/common-project.xml" />

	<fileset id="buildfiles.all">
		<!-- Files have to be in dependency order -->
		<include name="Castle.DynamicProxy/Castle.DynamicProxy.build" />
		<include name="Castle.DynamicProxy.Tests/Castle.DynamicProxy.Tests.build" if="${common.testrunner.enabled and not common.silverlight}"/>
	</fileset>

	<target name="build" depends="common.build" />
	
	<target name="generate-assemblyinfo" depends="common.assemblyinfo" />

</project>
